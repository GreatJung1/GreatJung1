#include<iostream>
#include<string>

using namespace std;
 
int x, y;      
int result;     

int main(void){
 

    int tmp=0;
    for(int i=0; i<9; i++){
        for(int j=0; j<9; j++){
            cin >> tmp;

            if(tmp > result){  
                y = i;
                x = j;
                result = tmp;
            }
        }
    }

     
    cout << result << endl;
    cout << y+1 << " " << x+1; 

    return 0;
}
 
